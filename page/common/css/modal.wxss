}

.help-result-button {
  margin-bottom: 40rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  animation: 0.3s font-animation infinite alternate;
}

@keyframes font-animation {
  0% {
    font-size: 24rpx;
  }

  45% {
    font-size: 24rpx;
  }

  55% {
    font-size: 30rpx;
  }

  100% {
    font-size: 30rpx;
  }
}

/* friend */

.friend-list-border {
  width: 560rpx;
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 0;
  overflow: hidden;
  transition: height 2s;
  -moz-transition: height 2s; /* Firefox 4 */
  -webkit-transition: height 2s; /* Safari and Chrome */
  -o-transition: height 2s;
}

.kind-list-item-bd-show {
  height: fit-content;
}

.friend-list-title {
  font-size: 30rpx;
  font-weight: bold;
  line-height: 30rpx;
  margin-top: 37rpx;
  margin-bottom: 50rpx;
  width: 100%;
   display: flex;
  justify-content: center;
  align-items: center;
}

.line1 {
  height: 1px;
  width: 100rpx;
  background-color: #e5e5e5;
}

.friend-list-title-text {
  font-size: 30rpx;
  padding: 0 20rpx;
  line-height: 30rpx;
}

.friend-list-item {
  height: 104rpx;
  width: 560rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 60rpx;
}

.friend-list-item:first-child .friend-item-left-border .friend-item-left-avatar {
