  align-items: center;
  justify-content: flex-end;
}

.friend-item-first-line {
  width: 474rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.friend-item-first-left {
  font-size: 30rpx;
  line-height: 30rpx;
}

.friend-item-first-right {
  font-size: 24rpx;
  color: #fd5d6f;
