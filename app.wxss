/* reset */

.navigationBar {
  display: flex;
  width: 750rpx;
  position: fixed;
  top: 0;
  left: 0;
  align-items: center;
  justify-content: center;
}

.close {
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: fixed;
  z-index: 0;
}

@font-face {
  font-family: 'Ionicons'; /* project id 771713 */
  src: url('//at.alicdn.com/t/font_771713_igrt98zj4xq.eot');
  src: url('//at.alicdn.com/t/font_771713_igrt98zj4xq.eot?#iefix') format('embedded-opentype'),
         url('//at.alicdn.com/t/font_771713_igrt98zj4xq.woff') format('woff'),
         url('//at.alicdn.com/t/font_771713_igrt98zj4xq.ttf') format('truetype'),
         url('//at.alicdn.com/t/font_771713_igrt98zj4xq.svg#Ionicons') format('svg');
}

.Ionicons {
  font-family: "Ionicons";
  font-size: 30rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
  padding: 0;
}

