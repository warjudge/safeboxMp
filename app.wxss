.scroll-view-with-footer {
  padding-bottom: 128rpx;
}

scroll-view {
  /*padding-bottom: 40rpx;*/
}

.footer {
  height: 98rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  /* justify-content: center; */
  align-items: center;
  background-color: #fff;
}

.card-border {
  width: 710rpx;
  margin-left: 20rpx;
  border-radius: 20rpx;
  background-color: #fff;
  margin-top: 30rpx;
  margin-bottom: 10rpx;
  box-shadow: 0px 2px 8px rgba(0, 0, 0, 0.03);
}

.card-border-large {
  width: 670rpx;
  margin-left: 40rpx;
  border-radius: 30rpx;
  background-color: #fff;
  margin-top: 30rpx;
  padding-bottom: 10rpx;
  box-shadow: 0px 2px 8px rgba(0, 0, 0, 0.03);
  margin-bottom: 30rpx;
}

.card-large-title-border {
  height: 88rpx;
  width: 670rpx;
  border-radius: 30rpx 30rpx 0 0;
  border-bottom: 1px solid#e5e5e5;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.card-large-title-left {
  font-size: 30rpx;
  margin-left: 30rpx;
}

.card-large-title-right {
  font-size: 24rpx;
  color: #999;
  margin-right: 30rpx;
}

.box-shadow {
  box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.06);
}

.custom-button {
  width: 590rpx;
  height: 80rpx;
  font-size: 30rpx;
  color: #fff;
  background-color: #cdb68a;
  border-radius: 20rpx;
  margin: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 80rpx;
}

.custom-button::after {
  border: none;
}

.gradient-button {
  background-image: linear-gradient(102deg, #cdb68a, #cdb68a);
}

.red-gradient-button {
  background-image: linear-gradient(102deg, #fd9b8a, #fd5d6f);
}

.standard-margin-top {
  margin-top: 30rpx;
}

.standard-margin-left {
  margin-left: 30rpx !important;
}

.large-margin-left {
  margin-left: 40rpx !important;
}

.standard-padding {
  padding-left: 40rpx;
  padding-right: 40rpx;
}

.large-margin-top {
  margin-top: 40rpx;
}

.small-margin-top {
  margin-top: 20rpx;
}

.modal-content {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}

.modal-content1 {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: transparent;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}

.button-text {
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-image: linear-gradient(102deg, #fd9b8a, #fd5d6f);
}

.button-text1 {
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-image: linear-gradient(102deg, #cdb68a, #cdb68a);
}

